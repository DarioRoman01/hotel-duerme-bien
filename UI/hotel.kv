#:import partial functools.partial

ScreenManager:
    LoginScreen:
        name: 'login'
    MainScreen:
        name: 'main'

<LoginScreen>:
    LoginLayout:
        Button:
            text: 'ingresar'
            font_size: 32
            on_press: self.parent.press(root.manager)
        

<MainScreen>:
    BoxLayout:
        orientation: 'vertical'
        Label:
            text: 'Main_Screen'
        Button:
            text: 'salir'
            font_size: 32
            on_press: root.manager.current = 'login'